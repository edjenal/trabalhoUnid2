		<managed-bean-class>managedbeans.ListarClienteBeans</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>NovoClienteBeans</managed-bean-name>
		<managed-bean-class>managedbeans.NovoClienteBeans</managed-bean-class>
