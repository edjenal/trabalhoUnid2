	<managed-bean>
		<managed-bean-name>EditarAtendimentoBeans</managed-bean-name>
		<managed-bean-class>managedbeans.EditarAtendimentoBeans</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>ListarAnamneseBeans</managed-bean-name>
		<managed-bean-class>managedbeans.ListarAnamneseBeans</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
    
</faces-config>
